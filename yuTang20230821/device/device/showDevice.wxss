/* pages/index/device/showDevice.wxss */
.page{
  padding: 0;
}
.video-grid-1 {
  display: grid;
  grid-template-columns: repeat(auto-fill, 1fr);
  gap: 1px;
}

.video-wrapper {
  position: relative;

  overflow: hidden;
}

.text_class {
  font-size: 16px;
  line-height: normal; /* 设置行高，可以根据具体需求微调 */
  font-family: '宋体', sans-serif; /* 设置字体为宋体 */

}

.video-container {
  width: 100%;
}

.page video {
  width: 100%;
}
