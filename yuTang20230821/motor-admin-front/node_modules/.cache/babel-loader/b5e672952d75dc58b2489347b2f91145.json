AG,CAACE,IAAJ,KAAa,GAAb,IAAoBF,GAAG,CAACE,IAAJ,KAAa,CAArC,EAAwC;AACtCxB,IAAAA,OAAO,CAAC;AACNyB,MAAAA,OAAO,EAAEH,GAAG,CAACI,GAAJ,IAAW,OADd;AAENC,MAAAA,IAAI,EAAE,OAFA;AAGNC,MAAAA,QAAQ,EAAE,IAAI;AAHR,KAAD,CAAP;;AAMA,QAAIN,GAAG,CAACE,IAAJ,KAAa,IAAb,IAAqBF,GAAG,CAACE,IAAJ,KAAa,IAAtC,EAA4C;AAC1C;AACAzB,MAAAA,UAAU,CAAC8B,OAAX,CACE,cADF,EAEE,MAFF,EAEU;AACNC,QAAAA,iBAAiB,EAAE,MADb;AAENC,QAAAA,gBAAgB,EAAE,IAFZ;AAGNJ,QAAAA,IAAI,EAAE;AAHA,OAFV,EAOEK,IAPF,CAOO,YAAM;AACX/B,QAAAA,KAAK,CAACgC,QAAN,CAAe,iBAAf,EAAkCD,IAAlC,CAAuC,YAAM;AAC3CE,UAAAA,QAAQ,CAACC,MAAT;AACD,SAFD;AAGD,OAXD;AAYD;;AACD,WAAOhB,OAAO,CAACC,MAAR,CAAe,IAAIgB,KAAJ,CAAUd,GAAG,CAACI,GAAJ,IAAW,OAArB,CAAf,CAAP;AACD,GAvBD,MAuBO;AACL,WAAOJ,GAAP;AACD;AAGF,CA5CH,EA6CE,UAAAN,KAAK,EAAI;AACPC,EAAAA,OAAO,CAACC,GAAR,CAAY,QAAQF,KAApB,EADO,CACqB;;AAC5BhB,EAAAA,OAAO,CAAC;AACNyB,IAAAA,OAAO,EAAET,KAAK,CAACU,GADT;AAENC,IAAAA,IAAI,EAAE,OAFA;AAGNC,IAAAA,QAAQ,EAAE,IAAI;AAHR,GAAD,CAAP;AAKA,SAAOT,OAAO,CAACC,MAAR,CAAeJ,KAAf,CAAP;AACD,CArDH;AAwDA,eAAed,OAAf","sourcesContent":["import axios from \"axios\";\nimport {\n  MessageBox,\n  Message\n} from \"element-ui\";\nimport store from \"@/store\";\n// import { getToken } from \"@/utils/auth\";\n\n// create an axios instance\nconst service = axios.create({\n  baseURL: process.env.VUE_APP_BASE_API, // url = base url + request url\n  // withCredentials: true, // send cookies when cross-domain requests\n  timeout: 5000 // request timeout\n});\n\n// request interceptor\nservice.interceptors.request.use(\n\n  config => {\n    // do something before request is sent\n    config.headers[\"access-token\"] = store.getters.token || \"\";\n    return config;\n  },\n  error => {\n    // do something with request error\n    console.log(error); // for debug\n    return Promise.reject(error);\n  }\n);\n\n// response interceptor\nservice.interceptors.response.use(\n  /**\n   * If you want to get http information such as headers or status\n   * Please return  response => response\n   */\n\n  /**\n   * Determine the request status by custom code\n   * Here is just an example\n   * You can also judge the status by HTTP Status Code\n   */\n  response => {\n    const res = response.data;\n    // console.log(res);\n\n    // if the custom code is not 20000, it is judged as an error.\n    if (res.code !== 200 && res.code !== 0) {\n      Message({\n        message: res.msg || \"Error\",\n        type: \"error\",\n        duration: 5 * 1000\n      });\n\n      if (res.code === 1002 || res.code === 1003) {\n        // to re-login\n        MessageBox.confirm(\n          \"登录已经过期，请重新登录\",\n          \"登录失效\", {\n            confirmButtonText: \"重新登录\",\n            cancelButtonText: \"取消\",\n            type: \"warning\"\n          }\n        ).then(() => {\n          store.dispatch(\"user/resetToken\").then(() => {\n            location.reload();\n          });\n        });\n      }\n      return Promise.reject(new Error(res.msg || \"Error\"));\n    } else {\n      return res;\n    }\n\n    \n  },\n  error => {\n    console.log(\"err\" + error); // for debug\n    Message({\n      message: error.msg,\n      type: \"error\",\n      duration: 5 * 1000\n    });\n    return Promise.reject(error);\n  }\n);\n\nexport default service;\n"]}]}