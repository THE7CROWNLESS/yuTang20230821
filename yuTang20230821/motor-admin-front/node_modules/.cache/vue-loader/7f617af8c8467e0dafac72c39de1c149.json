\n\t\t\t\tthis.$refs[formName].validate((valid) => {\r\n\t\t\t\t\tif (valid) {\r\n                        addMeal(this.form)\r\n                        .then((res) => {\r\n                            this.$message.success(\"创建成功\");\r\n                            this.loading = false;\r\n                            this.$router.push('/meal/index')\r\n                        })\r\n                        .catch((_) => {\r\n                            this.loading = false;\r\n                            this.$message.error(\"创建失败\");\r\n                        });\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tconsole.log('error submit!!');\r\n                        this.loading = false;\r\n\t\t\t\t\t\treturn false;\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\tresetForm(formName) {\r\n\t\t\t\tthis.$refs[formName].resetFields();\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n\t.el-form {\r\n        padding: 0 30px;\r\n\r\n\t\t.el-input, .el-input-number {\r\n\t\t\twidth: 300px;\r\n            margin-right: 20px;\r\n\t\t}\r\n\r\n\t\t.submit-item{\r\n\t\t\tmargin-top: 50px;\r\n\t\t}\r\n\t}\r\n</style>\r\n"]}]}